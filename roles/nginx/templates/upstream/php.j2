upstream cluster_php {
	{{ nginx_cluster_php_method }};
	{{% for item in nginx_cluster_servers %}}
	server {{ item.host }}:{{ item.port}} weight={{ item.weight | default('1') }} max_fails={{ item.max_fails | default('3') }} fail_timeout={{ item.fail_timeout | default('10s') }};
	{{% endfor %}}
}
